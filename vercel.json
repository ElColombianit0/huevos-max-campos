{
  "version": 2,
  "builds": [
      {
          "src": "app.py",
          "use": "@ardnt/vercel-python-wsgi",
          "config": { "maxLambdaSize": "15mb" }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "/"
      }
  ],
  "env": [
      "GOOGLE_CLIENT_ID",
      "GOOGLE_CLIENT_SECRET",
      "MONGO_URI"
  ]
}